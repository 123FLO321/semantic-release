  },
  "engines": {
    "iojs": "^1",
    "node": "^0.10",
    "npm": "^2"
  },
  "homepage": "https://github.com/boennemann/semantic-release",
  "keywords": [
    "author",
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version"
  ],
  "license": "MIT",
  "main": "dist/main.js",
  "optionalDependencies": {
    "babel": "^5.5.6"
  },
  "release": {
    "verification": "cracks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boennemann/semantic-release.git"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist && babel src --out-dir dist",
    "postpublish": "./bin/semantic-release.js post",
    "prepublish": "npm run build && ./bin/semantic-release.js pre",
    "pretest": "npm run build",
    "test": "./bin/test",
    "test:integration": "tap tests/{scenarios,tap}/*.js",
    "test:style": "standard"
  }
}
